{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "airline-ui": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/airline-ui",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/assets/img/user/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/css/user/bootstrap.min.css",
              "src/styles.css",
              "src/assets/css/user/font-awesome.css",
              "src/assets/css/user/font/flaticon.css",
              "src/assets/css/user/slick.css",
              "src/assets/css/user/ion.rangeSlider.min.css",
              "src/assets/css/user/datepicker.css",
              "src/assets/css/user/magnific-popup.css",
              "src/assets/css/user/nice-select.css",
              "src/assets/css/user/style.css",
              "src/assets/css/user/responsive.css",
              "src/assets/vendors/themefy_icon/themify-icons.css",
              "src/assets/vendors/scroll/scrollable.css",
              "src/assets/vendors/font_awesome/css/all.min.css",
              "src/assets/vendors/datatable/css/jquery.dataTables.min.css",
              "src/assets/vendors/datatable/css/responsive.dataTables.min.css",
              "src/assets/vendors/datatable/css/buttons.dataTables.min.css",
              "src/assets/css/admin/metisMenu.css",
              "src/assets/css/admin/style1.css",
              "./node_modules/ngx-toastr/toastr.css" ,
              "src/assets/css/admin/bootstrap1.min.css",
              "src/assets/vendors/themefy_icon/themify-icons.css",
              "src/assets/vendors/niceselect/css/nice-select.css",
              "src/assets/vendors/owl_carousel/css/owl.carousel.css",
              "src/assets/vendors/gijgo/gijgo.min.css",
              "src/assets/vendors/font_awesome/css/all.min.css",
              "src/assets/vendors/tagsinput/tagsinput.css",
              "src/assets/vendors/datepicker/date-picker.css",
              "src/assets/vendors/vectormap-home/vectormap-2.0.2.css",
              "src/assets/vendors/scroll/scrollable.css",
              "src/assets/vendors/datatable/css/jquery.dataTables.min.css",
              "src/assets/vendors/datatable/css/responsive.dataTables.min.css",
              "src/assets/vendors/datatable/css/buttons.dataTables.min.css",
              "src/assets/vendors/text_editor/summernote-bs4.css",
              "src/assets/vendors/morris/morris.css",
              "src/assets/vendors/material_icon/material-icons.css",
              "src/assets/css/admin/colors/default.css"
            ],
            "scripts": [
              "src/assets/js/user/jquery.min.js",
              "src/assets/js/user/popper.min.js",
              "src/assets/js/user/bootstrap.min.js",
              "src/assets/js/user/ion.rangeSlider.min.js",
              "src/assets/js/user/slick.min.js",
              "src/assets/js/user/datepicker.js",
              "src/assets/js/user/datepicker.en.js",
              "src/assets/js/user/isotope.pkgd.min.js",
              "src/assets/js/user/jquery.nice-select.js",
              "src/assets/js/user/jquery.magnific-popup.min.js",
              "src/assets/js/user/custom.js",
              "src/assets/js/admin/metisMenu.js",
              "src/assets/vendors/datatable/js/jquery.dataTables.min.js",
              "src/assets/vendors/datatable/js/dataTables.responsive.min.js",
              "src/assets/vendors/datatable/js/dataTables.buttons.min.js",
              "src/assets/vendors/datatable/js/buttons.flash.min.js",
              "src/assets/vendors/datatable/js/jszip.min.js",
              "src/assets/vendors/datatable/js/pdfmake.min.js",
              "src/assets/vendors/datatable/js/vfs_fonts.js",
              "src/assets/vendors/datatable/js/buttons.html5.min.js",
              "src/assets/vendors/datatable/js/buttons.print.min.js",
              "src/assets/vendors/scroll/perfect-scrollbar.min.js",
              "src/assets/js/admin/custom.js",
              "src/assets/js/admin/jquery1-3.4.1.min.js",
              "src/assets/js/admin/jquery1-3.4.1.min.js",
              "src/assets/js/admin/popper1.min.js",
              "src/assets/js/admin/bootstrap1.min.js",
              "src/assets/js/admin/metisMenu.js",
              "src/assets/vendors/count_up/jquery.waypoints.min.js",
              "src/assets/vendors/chartlist/Chart.min.js",
              "src/assets/vendors/count_up/jquery.counterup.min.js",
              "src/assets/vendors/niceselect/js/jquery.nice-select.min.js",
              "src/assets/vendors/owl_carousel/js/owl.carousel.min.js",
              "src/assets/vendors/datatable/js/jquery.dataTables.min.js",
              "src/assets/vendors/datatable/js/dataTables.responsive.min.js",
              "src/assets/vendors/datatable/js/dataTables.buttons.min.js",
              "src/assets/vendors/datatable/js/buttons.flash.min.js",
              "src/assets/vendors/datatable/js/jszip.min.js",
              "src/assets/vendors/datatable/js/pdfmake.min.js",
              "src/assets/vendors/datatable/js/vfs_fonts.js",
              "src/assets/vendors/datatable/js/buttons.html5.min.js",
              "src/assets/vendors/datatable/js/buttons.print.min.js",
              "src/assets/vendors/datepicker/datepicker.js",
              "src/assets/vendors/datepicker/datepicker.en.js",
              "src/assets/vendors/datepicker/datepicker.custom.js",
              "src/assets/js/admin/chart.min.js",
              "src/assets/vendors/chartjs/roundedBar.min.js",
              "src/assets/vendors/progressbar/jquery.barfiller.js",
              "src/assets/vendors/tagsinput/tagsinput.js",
              "src/assets/vendors/scroll/perfect-scrollbar.min.js",
              "src/assets/vendors/scroll/scrollable-custom.js",
              "src/assets/vendors/vectormap-home/vectormap-2.0.2.min.js",
              "src/assets/vendors/vectormap-home/vectormap-world-mill-en.js",
              "src/assets/js/admin/dashboard_init.js",
              "src/assets/js/admin/custom.js"

            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "airline-ui:build:production"
            },
            "development": {
              "browserTarget": "airline-ui:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "airline-ui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {

    "analytics": false

  }
}
